		"/asset/{domain}/{path}": {
			"get": {
				"tags": ["public-assets"],
				"summary": "Fetch a public asset",
				"description": "Public route for domain assets. The `{path}` parameter can contain slashes.",
				"security": [],
				"parameters": [
					{
						"name": "domain",
						"in": "path",
						"required": true,
						"schema": {
							"type": "string"
						},
						"description": "Domain name."
					},
					{
						"name": "path",
						"in": "path",
						"required": true,
						"allowReserved": true,
						"schema": {
							"type": "string"
						},
						"description": "Asset path under the domain assets directory (may contain `/`)."
					}
				],
				"responses": {
					"200": {
						"description": "Asset bytes.",
						"content": {
							"application/octet-stream": {
								"schema": {
									"type": "string",
									"format": "binary"
								}
							}
						}
					},
					"404": {
						"description": "Not found."
					},
					"500": {
						"description": "Server error."
					}
				}
			}
		},
